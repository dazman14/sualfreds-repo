<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.fredsrepo" name="Sualfreds Repo" version="1.0.2" provider-name="sualfred">
	<extension point="xbmc.addon.repository" name="Sualfreds Repository">
		<info compressed="false">https://raw.githubusercontent.com/sualfred/sualfreds-repo/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/sualfred/sualfreds-repo/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/sualfred/sualfreds-repo/master/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Sualfreds Repo for his noob Kodi things</summary>
		<description></description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>

<addon id="skin.bellofredo" version="4.0.3" name="Bello Fredo" provider-name="sualfred">
  <requires>
		<import addon="xbmc.gui" version="5.8.0" />
	</requires>
	<extension point="xbmc.gui.skin" defaultthemename="Textures.xbt" effectslowdown="0.75" debugging="false">
	    <res width="1280" height="720" aspect="16:9" default="true" folder="720p"/>
	</extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">A GUI for Kodi Media Center</summary>
    <description lang="en">Mod for Kodi 15 Nightlies of Nessus's Bello skin with some additional features</description>
    <disclaimer lang="en">Mouse usage is not supported</disclaimer>
    <platform>all</platform>
    <license>Creative Commons Attribution-NonCommercial-Share Alike 3.0 Unported License</license>
    <forum></forum>
    <email></email>
    <source></source>
  </extension>
</addon>
</addons>
